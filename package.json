{
  "name": "eep-movies",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "http-server": "^0.11.1",
    "random-text-meme": "^1.1.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "restify": "^8.4.0",
    "restify-cors-middleware": "^1.1.1",
    "rxjs": "^6.5.2",
    "server-health": "^3.1.0"
  },
  "scripts": {
    "start:dev": "concurrently \"react-scripts start\" \"node server.js\"",
    "start": "npm run build && pm2 stop eep-movies && pm2 start server.js --name eep-movies",
    "build": "npm run clean && react-scripts build && npm run dist",
    "clean": "rimraf build && rimraf dist",
    "dist": "mkdir dist && mv build/ dist/public && cp server.js dist/ && cp package.json dist/ && cp package-lock.json dist/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "rimraf": "^2.6.3"
  }
}
